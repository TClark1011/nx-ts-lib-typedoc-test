{
  "name": "My CLI Lib",
  "entryPoints": ["./packages/lib/src/functions/*/index.ts"],
  "outputs": [
    {
      "name": "html",
      "path": "./docs_gen/html"
    },
    {
      "name": "markdown",
      "path": "./docs_gen/md"
    }
  ],
  "tsconfig": "./packages/lib/tsconfig.json",
  "validation": {
    "notDocumented": true
  },
  "plugin": ["typedoc-plugin-markdown"],
  "cleanOutputDir": true,
  "readme": "none"
}
